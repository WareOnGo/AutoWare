{
  "name": "autoware-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
"dev": "concurrently \"npm run dev --workspace=autoware-web\" \"npm run dev --workspace=autoware-backend\"",
    "build": "npm run build --workspaces"
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  }
}